{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"printWidth": 160,
	"quoteProps": "consistent",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	],
	"svelteSortOrder": "options-scripts-markup-styles"
}